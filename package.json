{
    "name": "drageniix.github.io-server",
    "version": "1.0.0",
    "description": "Server side connection to Ashley Barrow's website.",
    "main": "index.js",
    "scripts": {
        "build": "tsc --build --clean && tsc",
        "heroku-postbuild": "npm run test",
        "test": "jest --config=jest.config.js --coverage",
        "start": "node build/server.js",
        "start:dev": "node -r ts-node/register ./src/server.ts",
        "start:watch": "nodemon"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drageniix/drageniix.github.io.git"
    },
    "types": "module",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/drageniix/drageniix.github.io/issues"
    },
    "engines": {
        "node": "10.16.2",
        "npm": "6.9.0"
    },
    "nodemonConfig": {
        "ignore": [
            "src/__tests__",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run start:dev",
        "ext": "ts"
    },
    "homepage": "https://github.com/drageniix/drageniix.github.io#readme",
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "express-validator": "^6.1.1",
        "socket.io": "^2.2.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/express": "^4.17.0",
        "@types/jest": "^24.0.17",
        "@types/node": "^12.7.2",
        "@types/socket.io": "^2.1.2",
        "@typescript-eslint/eslint-plugin": "^2.0.0",
        "@typescript-eslint/parser": "^2.0.0",
        "eslint": "^6.1.0",
        "eslint-plugin-jest": "^22.15.1",
        "jest": "^24.9.0",
        "jest-cli": "^24.9.0",
        "nodemon": "^1.19.1",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3"
    }
}
